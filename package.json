{
  "name": "@abisheks238/react-smart-table",
  "version": "1.0.1",
  "description": "ðŸš€ Enterprise-grade React table with AI autofill, real-time validation, and advanced security features. Perfect for admin dashboards and data management applications.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "SECURITY.md"
  ],
  "keywords": [
    "react",
    "table",
    "ai",
    "autofill",
    "validation",
    "smart-table",
    "data-grid",
    "admin-dashboard",
    "real-time",
    "export",
    "csv",
    "json",
    "typescript",
    "tailwind",
    "security",
    "enterprise",
    "production-ready",
    "xss-protection",
    "data-management",
    "filtering",
    "sorting",
    "pagination",
    "editable",
    "react-component",
    "ui-library"
  ],
  "author": {
    "name": "Abishek K",
    "email": "abisheks238@gmail.com",
    "url": "https://github.com/ABISHEK-K-DEV"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ABISHEK-K-DEV/react-smart-table.git"
  },
  "bugs": {
    "url": "https://github.com/ABISHEK-K-DEV/react-smart-table/issues"
  },
  "homepage": "https://github.com/ABISHEK-K-DEV/react-smart-table#readme",
  "scripts": {
    "build": "npm run clean && node scripts/build-esbuild.js || npm run build:fallback",
    "build:esbuild": "node scripts/build-esbuild.js",
    "build:rollup": "rollup -c",
    "build:fallback": "node scripts/build-fallback.js",
    "build:tsc": "tsc --project tsconfig.json",
    "dev": "npm run build:esbuild -- --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run lint:check && npm run type-check && npm run test && npm run build",
    "release": "npm run prepublishOnly && npm publish",
    "release:beta": "npm run prepublishOnly && npm publish --tag beta",
    "security:audit": "npm audit --audit-level moderate || echo \"Run npm install first to create package-lock.json\"",
    "security:fix": "npm install && npm audit fix || npm audit",
    "security:fix:force": "npm install && npm audit fix --force || npm audit",
    "example:install": "cd examples/basic && npm install",
    "example:dev": "cd examples/basic && npm run dev",
    "docs:build": "typedoc src/index.ts",
    "size": "size-limit",
    "analyze": "esbuild src/index.ts --bundle --minify --sourcemap --analyze --outfile=dist/bundle-analyze.js || rollup -c --bundleConfigAsCjs --environment ANALYZE",
    "build:check": "node scripts/build-check.js",
    "postinstall": "node scripts/build-check.js || echo 'Build check skipped'"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.11.2",
    "clsx": "^2.0.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.303.0",
    "papaparse": "^5.4.1",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.5",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.25.8",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "rollup": "^4.46.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.0.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "45 KB"
    }
  ],
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "^1.0.6",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ABISHEK-K-DEV"
  },
  "config": {
    "optional-dependencies": {
      "ignore-missing": true
    }
  }
}
